{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "plugins": [
      {
        "name": "typescript-styled-plugin"
      }
    ],
    "moduleResolution": "node",
    "importHelpers": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "allowJs": true,
    "module": "commonjs",
    "target": "ESNext"
  }
}
