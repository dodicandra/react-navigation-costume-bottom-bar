{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "importHelpers": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "allowJs": true,
    "module": "commonjs",
    "target": "ESNext",
    "baseUrl": "./src",
    "paths": {
      "components": [
        "./components"
      ]
    }
  },
}